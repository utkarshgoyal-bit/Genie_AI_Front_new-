{
  "name": "genie_ai",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "dev": "expo start --port 8400",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prebuild": "expo prebuild -p android",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "build:dev:android": "eas build -p android --profile development --clear-cache",
    "build:dev:android:local": "eas build -p android --profile development --clear-cache --local",
    "build:preview:android": "eas build -p android --profile preview --clear-cache",
    "build:preview:ios": "eas build -p  ios --profile preview --clear-cache",
    "build:preview:android:local": "eas build -p android --profile preview --clear-cache --local",
    "build:preview:ios:local": "eas build -p ios --profile preview --clear-cache --local",
    "build:production:android:local": "eas build -p android --profile production --clear-cache --local"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.11.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "expo": "^54.0.8",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.8",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.6",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-video": "~3.0.11",
    "expo-web-browser": "~15.0.7",
    "lottie-ios": "^4.5.1",
    "lottie-react-native": "^7.2.2",
    "lucide-react-native": "^0.544.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-video": "^6.16.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2",
    "yarn": "^1.22.22"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-keyboard-aware-scroll-view"
        ]
      }
    }
  },
  "overrides": {
    "expo-constants": "~18.0.9"
  }
}
